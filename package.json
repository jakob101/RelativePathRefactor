{
  "name": "fiximports",
  "displayName": "FixImports",
  "description": "Fix relative paths in import statements across a folder tree",
  "version": "0.0.1",
  "publisher": "jakob101",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fixImports"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.fixImports",
        "title": "Fix imports"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.fixImports",
          "group": "1_modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "glob": "^7.1.1",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
